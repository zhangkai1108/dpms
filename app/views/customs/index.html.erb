<h1>Listing customs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Linkman</th>
      <th>Mainman</th>
      <th>Level</th>
      <th>Linktel</th>
      <th>Maintel</th>
      <th>Customdesc</th>
      <th>Customlove</th>
      <th>Opername</th>
      <th>Addr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customs.each do |custom| %>
      <tr>
        <td><%= custom.name %></td>
        <td><%= custom.linkMan %></td>
        <td><%= custom.mainMan %></td>
        <td><%= custom.level %></td>
        <td><%= custom.linkTel %></td>
        <td><%= custom.mainTel %></td>
        <td><%= custom.customDesc %></td>
        <td><%= custom.customLove %></td>
        <td><%= custom.operName %></td>
        <td><%= custom.addr %></td>
        <td><%= link_to 'Show', custom %></td>
        <td><%= link_to 'Edit', edit_custom_path(custom) %></td>
        <td><%= link_to 'Destroy', custom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custom', new_custom_path %>
